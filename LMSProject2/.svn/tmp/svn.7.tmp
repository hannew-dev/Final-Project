	 	<typeAlias type="kr.or.ddit.user.vo.StudentVO" alias="studentVO"/>
	 	<typeAlias type="kr.or.ddit.user.vo.ProfessorVO" alias="professorVO"/>
	 	<typeAlias type="kr.or.ddit.user.vo.EmployeeVO" alias="employeeVO"/>
	 	<typeAlias type="kr.or.ddit.msg.vo.MsgVO" alias="msgVO"/>
	 	<typeAlias type="kr.or.ddit.board.vo.BoardVO" alias="boardVO"/>
	 	<typeAlias type="kr.or.ddit.board.vo.BoardAttachVO" alias="boardAttachVO"/>
	 	<typeAlias type="kr.or.ddit.board.vo.PaginationInfoVO" alias="pagingVO"/>
	 	<typeAlias type="kr.or.ddit.portlet.vo.PortletVO" alias="portletVO"/>
	 	
