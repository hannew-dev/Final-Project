<script>
	$(function () {
		$('td').on('click', function() {
			  var $span = $(this).find('span');
			  if ($span.hasClass('text-success')) {
			    $span.removeClass('text-success').addClass('text-danger');
			    $span.html('<i class="fa-regular fa-xmark"></i>');
			  } else if($span.hasClass('text-danger')){
			    $span.removeClass('text-danger');
			    $span.html(' ');
			  }else{
				  $span.addClass('text-success')
				  $span.html('<i class="fa-solid fa-check"></i>');
			  }
			});
		
		var cnt = $('.fa-solid.fa-check').length;
		$('#cnt').text(cnt);

	})
</script>
